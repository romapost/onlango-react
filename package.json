  "scripts": {
    "nodemon": "BABEL_ENV=node NODE_PATH=server nodemon -w server -w config -w src/actions ./server",
    "webpack": "BABEL_ENV=browser webpack --progress --watch",
    "browser-sync": "browser-sync start --port 3001 --files 'public/*.js' 'public/*.css' --proxy https://web --ws --reload-delay 1000 --no-open",
    "dev": "npm run nodemon & npm run webpack & npm run browser-sync",
    "build": "BABEL_ENV=browser NODE_ENV=production webpack --progress",
    "start": "BABEL_ENV=node NODE_ENV=production NODE_PATH=server node server",
    "test": "NODE_PATH=server node -r babel-register ${@:-test/**} | tap-spec",
    "create-teachers": "BABEL_ENV=node NODE_PATH=server node -r babel-register createFakeTeachers.js"
  },
